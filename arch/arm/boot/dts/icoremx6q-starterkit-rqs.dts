/dts-v1/;

#include "imx6q.dtsi"
#include "imx6qdl-icore-rqs.dtsi"

/ {
	model = "Engicam i.CoreM6 Quad/Dual qseven SOM";
	compatible = "fsl,imx6-icore-rqs", "fsl,imx6q";
	
	aliases {
		mmc1 = &usdhc4;
		mmc0 = &usdhc3;
		mmc2 = &usdhc1;
	};
};

&ldb {
	ipu_id = <0>;
	sec_ipu_id = <0>;
};

&mxcfb1 {
	status = "okay";
};

&mxcfb2 {
	status = "okay";
};

&sata {
        fsl,transmit-level-mV = <1025>;
        fsl,transmit-boost-mdB = <3330>;
        fsl,transmit-atten-16ths = <9>;
        fsl,receive-eq-mdB = <3000>;
        status = "okay";
};


&pcie {
	power-on-gpio = <&gpio3 19 0>;
	reset-gpio = <&gpio3 29 0>;
	status = "okay";
};

&i2c1 {
 
	lm75a@4F {
		compatible = "lm75a";
		reg = <0x4F>;
	};

};

&i2c2 {

	hdmi: edid@50 {
		compatible = "fsl,imx6-hdmi-i2c";
		reg = <0x50>;
	};

};


&i2c3 {

	codec: sgtl5000@0a {
		compatible = "fsl,sgtl5000";
		reg = <0x0a>;
		clocks = <&clks 201>;
		VDDA-supply = <&reg_2p5v>;
		VDDIO-supply = <&reg_3p3v>;
		VDDD-supply = <&reg_1p8v>;
	};

};

&iomuxc {

	hog {
		pinctrl_hog: hoggrp {
			fsl,pins = <
				MX6QDL_PAD_SD2_DAT0__GPIO1_IO15 0x1f059
				MX6QDL_PAD_EIM_D19__GPIO3_IO19 0x1f059 /* not used pin for ADV7180 driver compatibility */
			>;
		};
	};
};


